{
  "name": "depu",
  "version": "0.1.0",
  "description": "automatically update your dependencies",
  "main": "src/index.js",
  "bin": {
    "depu": "./bin/cli.js"
  },
  "scripts": {
    "release": "np",
    "lint": "eslint src",
    "test": "jest"
  },
  "keywords": [],
  "author": "Benjamin Seber <seber@synyx.de> (https://www.synyx.de)",
  "license": "MIT",
  "dependencies": {
    "columnify": "1.5.4",
    "execa": "1.0.0",
    "has-flag": "3.0.0",
    "listr": "0.14.3",
    "minimist": "1.2.0",
    "semver": "5.6.0"
  },
  "devDependencies": {
    "eslint": "5.15.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-prettier": "3.0.1",
    "jest": "24.3.1",
    "np": "4.0.2",
    "prettier": "1.16.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bseber/depu.git"
  },
  "bugs": {
    "url": "https://github.com/bseber/depu/issues"
  },
  "homepage": "https://github.com/bseber/depu#readme",
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/__mocks__/**"
    ]
  }
}
